function x_new = model(x,u,Dt, Vc)
    % parameters
    K
    tau_surge = 2;
    tau_yaw = 1;
    
    x_new = zeros(4,1);
    x_new(1) = x(1) + (x(4)*cosd(x(3))+Vc(1))*Dt;
    x_new(2) = x(2) + (x(4)*sind(x(3))+Vc(2))*Dt;
    
    % "dYnAmIcS"
    x_new(3) = x(3) + (Dt/tau_yaw) * (K * u(2) - x(3));
    x_new(4) = x(4) + (Dt/tau_surge) * (K * u(1) - x(4));
    
    %sus
    x_new = real(x_new); 
end